{
	"name": "Leetcode Assistant",
	"version": "0.1.0",
	"description": "Help you to save your time on leetcode",
	"author": "https://github.com/buikhacnam",
	"permissions": ["storage", "tabs"],
	"background": {
		"service_worker": "background.js"
	},
	"action": {
		"default_icon": {
			"16": "assets/ext-icon.png",
			"24": "assets/ext-icon.png",
			"32": "assets/ext-icon.png"
		},
		"default_title": "Leetcode Assistant",
		"default_popup": "popup.html"
	},
	"manifest_version": 3
}
